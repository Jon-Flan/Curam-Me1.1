<%- include ("../partials/header") %> <!--Call header from partial file -->

<div class="insideWrapper">
	<div class="pageTitle">
		<h4 class="detailsTitle">Tasks - To Do</h4>
	</div>
	<div class="TaskCard">
		<%results.forEach(function(results){ %>
			<div class="taskCardInner">
				<div class="taskCardInner1">
					<h5 class="taskDetail">Patient:<%=results.FirstName%> <%=results.LastName%></h5>
					<h5 class="taskDetail">Bed No:<%=results.Bed_No%></h5>
					<h5 class="taskDetail">Task:<%=results.Task %></h5>
					<h5 class="taskDetail">Time:<%= results.Time %></h5>
					<h5 class="taskDetail">Assigned to:<%=results.S_ID %></h5>
					<h5 class="taskDetail">Completed:<%= results.Completed %></h5>
				</div>
				<div class="taskCardInner2">
					<button class="taskBtn" onclick="window.location.href='/task/<%=results.Task_ID%>'">Do</button>
				</div>
			</div>
			
		<% }); %>
	</div>
</div>

<%- include ("../partials/footer") %>  <!--Call footer from partial file -->


